{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@ocean/tsconfig/web-app.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "#/*": ["./src/*"]
    },
    "types": ["vite/client", "@ocean/types/global", "element-plus/global"]
  },
  "references": [{ "path": "./tsconfig.node.json" }],
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.vue", "src/typings/*.d.ts"]
}
